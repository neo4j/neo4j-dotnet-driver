      <DocumentationSource sourceFile="Neo4j.Driver\Neo4j.Driver\bin\Release\netstandard2.0\Neo4j.Driver.dll" />
      <DocumentationSource sourceFile="Neo4j.Driver\Neo4j.Driver\bin\Release\netstandard2.0\Neo4j.Driver.xml" />
      <DocumentationSource sourceFile="Neo4j.Driver\Neo4j.Driver.Reactive\bin\Release\netstandard2.0\Neo4j.Driver.Reactive.dll" />
      <DocumentationSource sourceFile="Neo4j.Driver\Neo4j.Driver.Reactive\bin\Release\netstandard2.0\Neo4j.Driver.Reactive.xml" />
      <DocumentationSource sourceFile="Neo4j.Driver\Neo4j.Driver.Simple\bin\Release\netstandard2.0\Neo4j.Driver.Simple.dll" />
      <DocumentationSource sourceFile="Neo4j.Driver\Neo4j.Driver.Simple\bin\Release\netstandard2.0\Neo4j.Driver.Simple.xml" />
    </DocumentationSources>
    <BuildAssemblerVerbosity>OnlyWarningsAndErrors</BuildAssemblerVerbosity>
    <HelpFileFormat>Website</HelpFileFormat>
    <IndentHtml>False</IndentHtml>
    <KeepLogFile>True</KeepLogFile>
    <DisableCodeBlockComponent>False</DisableCodeBlockComponent>
    <CleanIntermediates>True</CleanIntermediates>
    <MissingTags>Summary, Parameter, Returns, AutoDocumentCtors, Namespace, TypeParameter, AutoDocumentDispose</MissingTags>
    <ApiFilter>
    </ApiFilter>
    <VisibleItems>InheritedMembers, InheritedFrameworkMembers, Protected, ProtectedInternalAsProtected</VisibleItems>
    <DriverVersion Condition="'$(DriverVersion)' == ''">4.0</DriverVersion>
    <HelpFileVersion>$(DriverVersion).0.0</HelpFileVersion>
    <MaximumGroupParts>2</MaximumGroupParts>
    <NamespaceGrouping>False</NamespaceGrouping>
    <SyntaxFilters>Standard</SyntaxFilters>
    <SdkLinkTarget>Blank</SdkLinkTarget>
    <RootNamespaceContainer>False</RootNamespaceContainer>
    <PresentationStyle>VS2013</PresentationStyle>
    <Preliminary>False</Preliminary>
    <NamingMethod>Guid</NamingMethod>
    <HelpTitle>Neo4j Bolt Driver $(DriverVersion) for .NET</HelpTitle>
    <CopyrightText>Copyright %28c%29 2002-2020 Neo4j</CopyrightText>
    <CopyrightHref>http://neo4j.com</CopyrightHref>
    <ContentPlacement>AboveNamespaces</ContentPlacement>
    <NamespaceSummaries>
      <NamespaceSummaryItem name="Neo4j.Driver" isDocumented="True">The base namespace for interaction with the Neo4j .NET Driver.</NamespaceSummaryItem>
    </NamespaceSummaries>
    <SaveComponentCacheCapacity>100</SaveComponentCacheCapacity>
    <PlugInConfigurations>

    <FrameworkVersion>.NET Standard 2.0</FrameworkVersion>
    <OutputPath>.\Docs\</OutputPath>
    <HtmlHelpName>Neo4j.Driver.Documentation</HtmlHelpName>
    <Language>en-US</Language>
